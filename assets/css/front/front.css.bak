@charset "UTF-8";


body {
    padding-top: 60px;
    padding-bottom: 40px;
  }
  .sidebar-nav {
    padding: 9px 0;
  }

  @media (max-width: 980px) {
    /* Enable use of floated navbar text */
    .navbar-text.pull-right {
      float: none;
      padding-left: 5px;
      padding-right: 5px;
    }
  }

/* -----------------------------------------------------
 * 前台容器和导航部分
 * ----------------------------------------------------- */
/* Custom container */
.container {
margin: 0 auto;
max-width: 1000px;
}
.container > hr {
margin: 60px 0;
}

/* Supporting marketing content */
.marketing {
margin: 60px 0;
}
.marketing p + h4 {
margin-top: 28px;
}


/* Customize the navbar links to be fill the entire space of the .navbar */
.container .navbar .navbar-inner {
padding: 0;
}
.container .navbar .nav {
margin: 0;
display: table;
width: 100%;
}
.container .navbar .nav li {
display: table-cell;
width: 1%;
float: none;
}
.container .navbar .nav li a {
font-weight: bold;
text-align: center;
border-left: 1px solid rgba(255,255,255,.75);
border-right: 1px solid rgba(0,0,0,.1);
}
.container .navbar .nav li:first-child a {
border-left: 0;
border-radius: 3px 0 0 3px;
}
.container .navbar .nav li:last-child a {
border-right: 0;
border-radius: 0 3px 3px 0;
}
.carousel-caption {
background: none;
}
.item img {
width: 100%;
height: 180px;
}
.carousel {
margin-bottom: 0;
}

/* -----------------------------------------------------
 * 运动馆首页列表样式
 * ----------------------------------------------------- */
div,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,form,fieldset,input,p,bloc
kquote,th,td,ins,img{margin:0;padding:0;word-wrap:break-word}

html{background:#f2f2f2}
body{font:12px/22px verdana,Arial,Helvetica,'宋体',sans-serif;color:#333}

dl {
  margin-bottom: 10px;
}

dt,
dd {
  line-height: 30px;
}

dt {
  font-weight: bold;
}

dd {
  margin-left: 10px;
}

.dl-horizontal {
  *zoom: 1;
}

.dl-horizontal:before,
.dl-horizontal:after {
  display: table;
  line-height: 0;
  content: "";
}

.dl-horizontal:after {
  clear: both;
}

.dl-horizontal dt {
  margin-left: 0px;
  float: left;
  width: 60px;
  overflow: hidden;
  clear: left;
  text-align: left;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.dl-horizontal dd {
  margin-left: 80px;
}


.ulist{margin-bottom:30px;font:12px/22px}
.ulist li{line-height:24px;padding:3px 0 5px 0; list-style:none; margin:0px} 
.ulist li a{white-space:nowrap}

.typeTitle{color:#999;position:absolute;left:0;top:0;line-height:24px}

.dlTy{padding-left:0px;position:relative}

.condition a{padding:3px 5px;margin:02px;color:#666}
.condition a.selected{background:#1885ae;color:#fff}
.condition a:hover{background:#1885ae;color:#fff;text-decoration:none}

.search_kuang{height:29px;margin:0 0 0 7px;display:inline}
.search_kuang .search_button input{background:#f5f5f5 url(/css/sport_wide/ico_glass.gif) 5px -65px no-repeat}

/* 全部/可预订 已选择*/
li.select{padding:0;border:2px solid #39c}
li.select a{background:#1885ae;color:#fff;border-top:0}

.sportList a{color:#333}
.sportList .title h2,.sportList .title h3{font-size:14px;font-weight:bold;line-height:26px;overflow:hidden}
.sportList .title h2,.sportList .title span{font:14px/26px verdana,Arial,'微软雅黑','宋体';font-weight:bold}
/*effectLi让图片左对齐，并且去掉列表前面的点*/
.sportList ul li.effectLi{padding:0 0 20px;border-bottom:1px solid #f4f4f4;margin-bottom:20px;position:relative;display:inline-block}
.effectLi .ui_text{color:#999}
.sportList .ui_pic span.mark{width:61px;height:48px;background-position:-75px -42px}
.sportList .ui_text p b{font-size:12px;margin-right:10px;color:#1885ae}

.ui_media,.ui_text{*overflow:visible;zoom:1}
.ui_media .ui_pic img,.ui_media .ui_picR img{display:block}
.ui_media .ui_pic{margin-right:10px;float:left}
.ui_media .ui_picR{margin-left:10px;float:right}
.ui_media .ui_media{margin-top:10px;color:#666}
.ffst{font-family:'宋体'}
.mt10{margin-top:10px}

.grade{margin:0 0 0 9px;font:12px/22px verdana,Arial,Helvetica,'宋体',sans-serif}
.grade sub{font-size:24px;color:#f60;font-style:italic;vertical-align:middle}
.grade sup{color:#f60;font-size:14px;font-style:italic;line-height:20px;margin-left:-4px;vertical-align:3px}

.cSport{color:#39c!important}


#page{font-size:12px;height:21px;width:auto;margin:20px auto;float:right}
#page a,#page strong{float:left;margin:0 1px}
#page a{display:inline-block;height:21px;line-height:21px;padding:0 8px}
#page a:hover{background:#f60;color:#fff;text-decoration:none}
#page .on,#page a.on:hover{background:#c30;color:#fff}
#page a{color:#39c}
#page a:hover{background:#3ae}
#page .on,#page a.on:hover{background:#1885ae}