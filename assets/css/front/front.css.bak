@charset "UTF-8";


body {
    padding-top: 60px;
    padding-bottom: 40px;
  }
  .sidebar-nav {
    padding: 9px 0;
  }

  @media (max-width: 980px) {
    /* Enable use of floated navbar text */
    .navbar-text.pull-right {
      float: none;
      padding-left: 5px;
      padding-right: 5px;
    }
  }

/* -----------------------------------------------------
 * 前台容器和导航部分
 * ----------------------------------------------------- */
/* Custom container */
.container {
margin: 0 auto;
max-width: 1000px;
}
.container > hr {
margin: 60px 0;
}

/* Supporting marketing content */
.marketing {
margin: 60px 0;
}
.marketing p + h4 {
margin-top: 28px;
}


/* Customize the navbar links to be fill the entire space of the .navbar */
.container .navbar .navbar-inner {
padding: 0;
}
.container .navbar .nav {
margin: 0;
display: table;
width: 100%;
}
.container .navbar .nav li {
display: table-cell;
width: 1%;
float: none;
}
.container .navbar .nav li a {
font-weight: bold;
text-align: center;
border-left: 1px solid rgba(255,255,255,.75);
border-right: 1px solid rgba(0,0,0,.1);
}
.container .navbar .nav li:first-child a {
border-left: 0;
border-radius: 3px 0 0 3px;
}
.container .navbar .nav li:last-child a {
border-right: 0;
border-radius: 0 3px 3px 0;
}
.carousel-caption {
background: none;
}
.item img {
width: 100%;
height: 180px;
}
.carousel {
margin-bottom: 0;
}

/* -----------------------------------------------------
 * 运动馆首页列表样式
 * ----------------------------------------------------- */

/*
html,body,div,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,form,fieldset,input,p,bloc
kquote,th,td,ins,img{margin:0;padding:0;word-wrap:break-word}
*/
div,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,form,fieldset,input,p,bloc
kquote,th,td,ins,img{margin:0;padding:0;word-wrap:break-word}

html{background:#f2f2f2}
body{font:12px/22px verdana,Arial,Helvetica,'宋体',sans-serif;color:#333}

dl {
  margin-bottom: 10px;
}

dt,
dd {
  line-height: 30px;
}

dt {
  font-weight: bold;
}

dd {
  margin-left: 10px;
}

.dl-horizontal {
  *zoom: 1;
}

.dl-horizontal:before,
.dl-horizontal:after {
  display: table;
  line-height: 0;
  content: "";
}

.dl-horizontal:after {
  clear: both;
}

.dl-horizontal dt {
   margin-left: 0px;
  float: left;
  width: 60px;
  overflow: hidden;
  clear: left;
  text-align: left;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.dl-horizontal dd {
  margin-left: 80px;
}


.ulist{margin-bottom:30px;font:12px/22px}
.ulist li{line-height:24px;padding:3px 0 5px 0; list-style:none; margin:0px} 
.ulist li a{white-space:nowrap}

.typeTitle{color:#999;position:absolute;left:0;top:0;line-height:24px}

.dlTy{padding-left:0px;position:relative}

.condition a{padding:3px 5px;margin:02px;color:#666}
.condition a.selected{background:#1885ae;color:#fff}
.condition a:hover{background:#1885ae;color:#fff;text-decoration:none}

